		"stylelint-config-standard": "^36.0.0",
		"tailwindcss": "^3.4.4",
		"viem": "^2.15.1",
		"wagmi": "^2.10.3",
		"zod": "^3.23.8"
	},
	"devDependencies": {
		"@types/jest": "^29.5.12",
		"@types/node": "^20.14.6",
